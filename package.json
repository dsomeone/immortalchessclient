{
	"name": "client",
	"productName": "client",
	"version": "0.0.0",
	"description": "My transcendent app",
	"license": "MIT",
	"repository": "someone/client",
	"author": {
		"name": "Andrij David",
		"email": "andrijdavid@gmail.com",
		"url": "someone.hto"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --linux --windows",
		"release": "np"
	},
	"dependencies": {
		"@deadcanaries/granax": "^3.2.5",
		"@trodi/electron-splashscreen": "^1.0.0",
		"axios": "^0.21.1",
		"electron-context-menu": "^0.15.0",
		"electron-debug": "^3.0.0",
		"electron-proxy-agent": "^1.2.0",
		"electron-splashscreen": "^1.0.0",
		"electron-ssl-pinning": "^1.0.0",
		"electron-store": "^5.1.0",
		"electron-unhandled": "^3.0.0",
		"electron-updater": "^4.0.6",
		"electron-util": "^0.13.0",
		"find-open-port": "^2.0.0",
		"forever": "^2.0.0",
		"forever-monitor": "^2.0.0",
		"portfinder": "^1.0.26",
		"socks-proxy-agent": "^5.0.0",
		"tor-router": "^4.0.13"
	},
	"devDependencies": {
		"electron": "^7.1.1",
		"electron-builder": "^21.2.0",
		"np": "^5.0.3",
		"xo": "^0.25.3"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "com.someone.client",
		"mac": {
			"category": "public.app-category.social-networking",
			"darkModeSupport": true
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Network;Chat"
		}
	}
}
